<h1>Other Users:</h1>
<ul>
    <% @other_users.each do |user| %>
        <li><%= user.username %>
        <% if @current_user.followings.include?(user) %>
            <% fol = Follow.find_by(followed_id: user.id, follower_id: @current_user.id) %>
            <%= button_to "Unfollow", follow_path(fol), method: :delete %>
        <% else %>
            <%= button_to "Follow", follows_path(follower_id: @current_user.id, followed_id: user.id) %>
        <% end %>
        </li>
    <% end %>
</ul>
